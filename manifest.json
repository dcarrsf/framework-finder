{
  "manifest_version": 2,

  "name": "Detectinator",
  "description": "This extension analyzes front-end technologies used on the page.",
  "version": "1.0",
  "background": {
    "page": "background.html"
  },

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"]
    }
  ],

  "page_action": {},

  "permissions": [
    "activeTab"
  ]
}